{
	"root": true,
	"extends": "eslint:recommended",
	"rules": {
	  "func-style": [2, "declaration"],
	  "id-length": 0,
	  "multiline-comment-style": 0,
	  "new-cap": [2, {
		"capIsNewExceptions": ["GetIntrinsic"]
	  }],
	  "no-console": "warn",
	  "no-unused-vars": "warn",
	  "eqeqeq": "error",
	  "semi": [2, "always"]
	}
  }
  